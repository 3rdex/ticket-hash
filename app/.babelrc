{
  "presets": [
    "babel-preset-expo"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        "transform-class-properties",
        [
          "module-resolver",
          {
            "alias": {
              "stream": "./vendors/stream",
              "bytebuffer": "./vendors/bytebuffer",
              "crypto": "./vendors/crypto"
            }
          }
        ]
      ]
    }
  }
}
